SELECT * FROM orders WHERE order_date = '2023-03-18'
SELECT * FROM orders WHERE order_date = '2023-03-18' AND first_name =‘John’ AND last_name = ‘Doe’
SELECT AVG(amount) FROM customers WHERE purchase_date BETWEEN '2023-01-01' AND '2023-01-31'
SELECT * FROM departments WHERE sales > 600 AND （date BETWEEN '2022-01-01' AND '2022-12-31'）
SELECT MAX(revenue) AS most, MIN(revenue) AS least FROM orders
SELECT MAX(profict) AS most_lucrative FROM orders
